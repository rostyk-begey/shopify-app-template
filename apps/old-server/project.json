{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/old-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/old-server",
        "main": "apps/old-server/src/main.ts",
        "tsConfig": "apps/old-server/tsconfig.app.json",
        "assets": ["apps/old-server/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "old-server:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "old-server:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/old-server/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/old-server"],
      "options": {
        "jestConfig": "apps/old-server/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
